{
  "name": "agentary-js-sdk",
  "version": "1.0.0",
  "description": "JavaScript SDK for Agentary - designed for web browsers",
  "main": "dist/agentary.js",
  "module": "dist/agentary.esm.js",
  "browser": "dist/agentary.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "docs": "jsdoc -c jsdoc.conf.json",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build:prod"
  },
  "keywords": [
    "agentary",
    "sdk",
    "javascript",
    "browser",
    "web"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/agentary-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/agentary-js-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/agentary-js-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdoc": "^4.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.25.0"
  },
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
} 