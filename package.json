{
  "name": "agentary-js-sdk",
  "version": "0.0.1",
  "type": "module",
  "description": "JavaScript SDK for Agentary",
  "main": "dist/agentary.js",
  "module": "dist/agentary.esm.js",
  "browser": "dist/agentary.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "dev": "rollup -c -w",
    "serve": "python3 -m http.server 8080 || python -m http.server 8080 || npx http-server -p 8080",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{js,ts}",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "docs": "jsdoc -c jsdoc.conf.json",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run type-check && npm run build:prod",
    "cdn:build": "npm run build:prod && node cloudflare/build.js",
    "cdn:deploy": "bash cloudflare/deploy.sh",
    "cdn:deploy:prod": "bash cloudflare/deploy.sh production"
  },
  "keywords": [
    "agentary",
    "sdk",
    "javascript",
    "browser",
    "web"
  ],
  "author": "Declan Healey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/agentary-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/agentary-js-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/agentary-js-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/mixpanel-browser": "^2.60.0",
    "@types/node": "^22.15.30",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdoc": "^4.0.0",
    "postcss": "^8.5.4",
    "postcss-modules": "^6.0.1",
    "rimraf": "^5.0.0",
    "rollup": "^3.25.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mlc-ai/web-llm": "^0.2.79",
    "@preact/compat": "^18.3.1",
    "marked": "^15.0.12",
    "mixpanel-browser": "^2.65.0",
    "preact": "^10.26.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
